<%
var srcPath = '../../';
%>
<!doctype html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>お問い合わせ | monoweb.jp</title>
    <%- include(srcPath + 'partials/head'); %>
    <link href="https://fonts.googleapis.com/css?family=Noto+Sans+JP" rel="stylesheet">
    <link rel="stylesheet" href="/assets/css/style.css">
</head>
<body>
<div class="mjpl-page">
    <%- include(srcPath + 'partials/header'); %>
    <div class="mjpl-content">
        <main id="app">
            <form action="">
                <label for="">
                    <span>法人名・個人名</span>
                    <input type="text" name="" id="">
                </label>
                <label for="">
                    <span>メールアドレス</span>
                    <input type="email" name="" id="">
                </label>
                <label for="">
                    <span>お問い合わせ内容</span>
                    <textarea name="" id="" cols="30" rows="10"></textarea>
                </label>
                <%- include(srcPath + 'objects/components/button', {data: [
                {
                link: false,
                name: '送信',
                func: 'submit'
                }
                ]}); %>
            </form>
        </main>
    </div>
    <%- include(srcPath + 'partials/footer', {
    next: {
    name: "サービス内容",
    link: "/services/"
    }
    }); %>
</div>
<script src="/assets/js/bundle.js"></script>
</body>
</html>