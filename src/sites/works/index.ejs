<%
var srcPath = '../../';
%>
<!doctype html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>制作実績 | monoweb.jp</title>
    <%- include(srcPath + 'partials/head'); %>
    <link href="https://fonts.googleapis.com/css?family=Noto+Sans+JP" rel="stylesheet">
    <link rel="stylesheet" href="/assets/css/style.css">
</head>
<body>
<div class="mjpl-page">
    <%- include(srcPath + 'partials/header'); %>
    <div class="mjpl-content">
        <main id="app">
            <%- include(srcPath + 'objects/components/heading', {data: [{
            lv: 1,
            tag: 1,
            name: '制作実績',
            utility: 'mjpu-taCenter'
            }]}); %>
            <p class="mjpc-paragraph mjpu-fsS mjpu-taCenter">クライアント名は伏せさせていただいております。</p>
            <%- include(srcPath + 'objects/components/tag', {data: [
            {
            name: 'Webデザイン'
            }, {
            name: 'フロントエンド構築'
            }, {
            name: 'バックエンド構築'
            }, {
            name: 'ロゴデザイン'
            }, {
            name: '名刺デザイン'
            }
            ]}); %>
            <%- include(srcPath + 'objects/components/heading', {data: [
            {
            lv: 2,
            tag: 2,
            name: 2020
            }
            ]}); %>
            <div class="mjpp-workAccordion">
                <button aria-expanded="true" aria-controls="workAccordionContent0" id="workAccordionTrigger0" class="mjpp-workAccordion__trigger">
                    <span class="mjpp-workAccordion__image">
                        <img src="/img/dummy-600x400.png" alt="">
                    </span>
                    <span class="mjpp-workAccordion__text">
                        <span class="mjpp-workAccordion__title">ユーザー管理サービスUIリニューアル</span>
                        <span class="mjpp-workAccordion__tagList">
                            <span class="mjpp-workAccordion__tagItem">Webデザイン</span>
                            <span class="mjpp-workAccordion__tagItem">フロントエンド構築</span>
                            <span class="mjpp-workAccordion__tagItem">バックエンドエンド構築</span>
                        </span>
                        <span class="mjpp-workAccordion__description">
                            3Dパースモデリングの集客を目的としたランディングページのデザイン。
                        </span>
                    </span>
                </button>
                <div role="region" aria-labelledby="workAccordionTrigger0" id="workAccordionContent0" class="mjpp-workAccordion__content">
                    <div class="mjpc-grid">
                        <div class="mjpc-grid__inner">
                            <div class="mjpc-grid__col6">
                                <%- include(srcPath + 'objects/components/heading', {data: [
                                {
                                lv: 3,
                                tag: 3,
                                name: 'チーム編成'
                                }
                                ]}); %>
                                <dl>
                                    <dt>ディレクター</dt>
                                    <dd>1名</dd>
                                </dl>
                                <dl>
                                    <dt>デザイナー</dt>
                                    <dd>1名</dd>
                                </dl>
                                <dl>
                                    <dt>フロントエンド開発</dt>
                                    <dd>1名</dd>
                                </dl>
                                <dl>
                                    <dt>バックエンド開発</dt>
                                    <dd>3名</dd>
                                </dl>
                            </div>
                            <div class="mjpc-grid__col6">
                                <%- include(srcPath + 'objects/components/heading', {data: [
                                {
                                lv: 3,
                                tag: 3,
                                name: 'アプリケーション'
                                }
                                ]}); %>
                                <ul>
                                    <li></li>
                                    <li></li>
                                    <li></li>
                                    <li></li>
                                    <li></li>
                                </ul>
                                <%- include(srcPath + 'objects/components/heading', {data: [
                                {
                                lv: 3,
                                tag: 3,
                                name: '言語・フレームワーク'
                                }
                                ]}); %>
                            </div>
                        </div>
                    </div>
                    <%- include(srcPath + 'objects/components/heading', {data: [
                    {
                    lv: 3,
                    tag: 3,
                    name: '概要'
                    }
                    ]}); %>
                </div>
            </div>
            <%- include(srcPath + 'objects/components/heading', {data: [
            {
            lv: 2,
            tag: 2,
            name: 2019
            }
            ]}); %>
        </main>
    </div>
    <%- include(srcPath + 'partials/footer', {
    next: {
    name: "お問い合わせ",
    link: "/contact/"
    }
    }); %>
</div>
<script src="/assets/js/bundle.js"></script>
</body>
</html>