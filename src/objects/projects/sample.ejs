    var data = sample.button;
}
%>
<div>
    <h1><%= title %></h1>
    <ul>
        <% data.forEach(function (item) { %>
            <li>
                <h2><%= item.name %></h2>
                <p><%= item.email %></p>
            </li>
        <% }); %>
    </ul>
</div>