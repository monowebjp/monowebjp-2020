<%
if (typeof data === 'undefined') {
    var data = [{
        "lv": "{{lv}}",
        "tag": "{{tag}}",
        "name": "{{name}}",
        "class": "{{class}}"
    }]

    var styleguide = true;
}
data.forEach(function (item) {
if (styleguide) {
%>
    {{#each data}}
<% } %>
<h<%= item.tag %> class="mjpc-heading mjpc-heading--lv<%= item.lv %> <%= item.class %>"><%= item.name %></h<%= item.tag %>>
<% if (styleguide) { %>
    {{/each}}
<% } %>
<% }); %>